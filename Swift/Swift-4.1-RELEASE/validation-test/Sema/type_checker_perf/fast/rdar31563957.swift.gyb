// RUN: %scale-test --begin 1 --end 5 --step 1 --select incrementScopeCounter %s
// REQUIRES: OS=macosx
// REQUIRES: asserts

struct rdar33511986 {
  func test(s: String, b: Bool, os: String?, oa: Any?, o: AnyObject) -> [Any] {
    let a: [Any]
    a = [
%for i in range(0, N):
      [
	"first": s,
        "second": b,
        "third": os,
        "fourth": oa ?? o
      ],
%end
      [
	"first": s,
        "second": b,
        "third": nil,
        "fourth": oa ?? o
      ],
      [
	"first": "literal",
        "second": true,
        "third": nil,
        "fourth": nil
      ]
    ]

    return a
  }
}
