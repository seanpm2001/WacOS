enum Operation {
  case binaryOperation((Double, Double) -> Double)
}

var operations: [String: Operation] = [
  "+" : .binaryOperation({ $0 + $1 }),
%for i in range(1, N):
  "-" : .binaryOperation({ $0 - $1 }),
  "ร" : .binaryOperation({ $0 * $1 }),
  "รท" : .binaryOperation({ $0 / $1 }),
%end
]
