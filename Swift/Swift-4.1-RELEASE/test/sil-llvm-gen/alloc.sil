// REQUIRES: CODEGENERATOR=X86
// REQUIRES: CODEGENERATOR=ARM

import Builtin

struct Pair<T> {
  var fst: T, snd: T
}

struct Huge {
  var pairs: Pair<Pair<Pair<Pair<Pair<Pair<Pair<Pair<Pair<Builtin.Int64>>>>>>>>>
  var unalign: Builtin.Int8
}

