    switch self {
    case .native:
      nativeRemoveAll()
    case .cocoa(let cocoaStorage):
#if _runtime(_ObjC)
      self = .native(NativeStorage.Owner(minimumCapacity: cocoaStorage.count))
#else
      _sanityCheckFailure("internal error: unexpected cocoa ${Self}")
#endif
    }
  }

