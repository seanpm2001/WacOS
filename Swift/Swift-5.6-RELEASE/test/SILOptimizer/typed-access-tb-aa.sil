sil @builtin_test : $@convention(thin) () -> () {
bb0:
  %0 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.RawPointer>
  %1 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.NativeObject>
  %2 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.AnyObject>
  %3 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.Int8>
  %4 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.Int32>
  %5 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE32>
  %6 = alloc_box $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE64>

  %7 = project_box %0 : $<τ_0_0> { var τ_0_0 } <Builtin.RawPointer>, 0
  %8 = project_box %1 : $<τ_0_0> { var τ_0_0 } <Builtin.NativeObject>, 0
  %9 = project_box %2 : $<τ_0_0> { var τ_0_0 } <Builtin.AnyObject>, 0
  %10 = project_box %3 : $<τ_0_0> { var τ_0_0 } <Builtin.Int8>, 0
  %11 = project_box %4 : $<τ_0_0> { var τ_0_0 } <Builtin.Int32>, 0
  %12 = project_box %5 : $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE32>, 0
  %13 = project_box %6 : $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE64>, 0

  // This second block ensures that we gain the benefits of not repeating
  // already known values with being able to have associated with the output for
  // each type each of the items we are trying to check aliasing against. This
  // makes it simpler to write these tests.
  %14 = project_box %0 : $<τ_0_0> { var τ_0_0 } <Builtin.RawPointer>, 0
  %15 = project_box %1 : $<τ_0_0> { var τ_0_0 } <Builtin.NativeObject>, 0
  %16 = project_box %2 : $<τ_0_0> { var τ_0_0 } <Builtin.AnyObject>, 0
  %17 = project_box %3 : $<τ_0_0> { var τ_0_0 } <Builtin.Int8>, 0
  %18 = project_box %4 : $<τ_0_0> { var τ_0_0 } <Builtin.Int32>, 0
  %19 = project_box %5 : $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE32>, 0
  %20 = project_box %6 : $<τ_0_0> { var τ_0_0 } <Builtin.FPIEEE64>, 0

  %21 = tuple()
  return %21 : $()
}

